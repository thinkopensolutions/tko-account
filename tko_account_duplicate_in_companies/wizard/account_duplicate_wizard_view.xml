<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="duplicate_accounts_wizard_form" model="ir.ui.view">
        <field name="name">Duplicate Accounts</field>
        <field name="model">duplicate.accounts</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="company_ids" widget="many2many_tags"/>
                </group>
                <footer>
                    <button string="Duplicate"
                            class="oe_highlight"
                            name="duplicate_accounts" type="object"/>
                    <button string="Close"
                            class="oe_link"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>